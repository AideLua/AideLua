{
  CoordinatorLayout;
  id="mainLay";
  layout_height="fill";
  layout_width="fill";
  {
    rawget(_ENV,"FastScrollScrollView") or ScrollView;
    layout_height="fill";
    layout_width="fill";
    id="scrollView";
    fillViewport=true;
    {
      MaterialTextView;
      padding="16dp";
      id="textView";
      layout_height="fill";
      layout_width="fill";
      textIsSelectable=true;
      textColor=res.color.attr.colorOnBackground;
    };
  };
};
