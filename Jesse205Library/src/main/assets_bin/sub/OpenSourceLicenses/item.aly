{
  LinearLayoutCompat;
  layout_width="fill";
  orientation="vertical";
  {
    MaterialCardView;
    layout_margin="8dp";
    layout_width="fill";
    id="cardView";
    {
      LinearLayoutCompat;
      layout_height="fill";
      layout_width="fill";
      id="cardViewChild";
      {
        LinearLayoutCompat;
        layout_height="fill";
        layout_width="fill";
        layout_margin="16dp";
        orientation="vertical";
        {
          MaterialTextView;
          textSize="16sp";
          id="name";
          layout_marginBottom="8dp";
          textColor=res.color.attr.colorPrimary;
          --typeface=Typeface.defaultFromStyle(Typeface.BOLD);
        };
        {
          MaterialTextView;
          textColor=res.color.attr.colorOnBackground;
          id="message";
          textSize="14sp";
        };
        {
          MaterialTextView;
          id="license";
          textSize="14sp";
        };
      };
    };
  };
};
